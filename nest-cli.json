{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": true,
  "compilerOptions": {
    "tsConfigPath": "tsconfig.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "entryFile": "api"
    },
    "cli": {
      "type": "application",
      "entryFile": "cli"
    },
    "worker": {
      "type": "application",
      "entryFile": "worker"
    },
    "scheduler": {
      "type": "application",
      "entryFile": "scheduler"
    }
  }
}
