{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "NestJS Template",
  "author": "proscom.ru",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "prettify": "prettier --write \"src/**/*.ts\"",
    "prepare": "is-ci || husky install",
    "prebuild": "rimraf dist",
    "postinstall": "is-ci || husky install",
    "mock": "dotenv graphql-faker --forward-headers Authorization --extend \"http://$APP_HOST:$APP_PORT/graphql\" ./mock.graphql",
    "build": "nest build",
    "build:diagram": "ts-node ./node_modules/@proscom/typeorm-uml/bin/run src/config/ormconfig.ts --uml -n tables -n entities > diagram.puml",
    "start": "nest start",
    "start:dev": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/bin/app",
    "cli": "cross-env TYPEORM_BASE_DIRECTORY=src node -r tsconfig-paths/register -r ts-node/register src/bin/cli.ts",
    "cli:dev": "cross-env TYPEORM_BASE_DIRECTORY=src node --inspect -r ts-node/register src/bin/cli.ts",
    "cli:prod": "cross-env NODE_ENV=production node dist/bin/cli.js",
    "worker:dev": "cross-env TYPEORM_BASE_DIRECTORY=src node -r ts-node/register src/bin/worker.ts",
    "worker:prod": "cross-env NODE_ENV=production node dist/bin/worker.js",
    "scheduler:dev": "cross-env TYPEORM_BASE_DIRECTORY=src node -r ts-node/register src/bin/scheduler.ts",
    "scheduler:prod": "cross-env NODE_ENV=production node dist/bin/scheduler.js",
    "typeorm": "cross-env TYPEORM_BASE_DIRECTORY=src node -r ts-node/register ./node_modules/typeorm/cli.js -f src/config/ormconfig.ts",
    "typeorm:prod": "cross-env NODE_ENV=production node ./node_modules/typeorm/cli.js -f dist/config/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "@proscom/graphql-faker": "^2.0.0-rc.23",
    "@types/bull": "^3.15.0",
    "apollo-server-express": "^2.21.0",
    "bcrypt": "^5.0.0",
    "bull": "^3.22.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "nanoid": "^3.1.20",
    "nestjs-command": "^2.0.0",
    "nestjs-pino": "^1.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pino": "^6.11.1",
    "pino-pretty": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.31",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@proscom/typeorm-uml": "^1.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.25",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,scss,css,json}": [
      "prettier --write"
    ]
  }
}
