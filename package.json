{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "NestJS Template",
  "author": "proscom.ru",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettify": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:diagram": "ts-node ./node_modules/@proscom/typeorm-uml/bin/run src/config/ormconfig.ts --uml -n tables -n entities > diagram.puml",
    "build:schema": "echo \"This script will build GraphQL schema file\"",
    "start": "nest start",
    "start:dev": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/bin/app",
    "cli": "node -r tsconfig-paths/register -r ts-node/register src/bin/cli.ts",
    "cli:dev": "node --inspect -r tsconfig-paths/register -r ts-node/register src/bin/cli.ts",
    "cli:prod": "cross-env NODE_ENV=production node dist/bin/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "nestjs-command": "^2.0.0",
    "nestjs-pino": "^1.3.0",
    "pg": "^8.5.1",
    "pino": "^6.11.1",
    "pino-pretty": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.31",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@proscom/typeorm-uml": "^1.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.25",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
